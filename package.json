    "elliptic": "^6.5.4",
    "fetch": "^1.1.0",
    "loglevel": "^1.8.1",
    "web3-eth-contract": "^4.1.0",
    "web3-utils": "^4.0.6"
  },
  "devDependencies": {
    "@babel/register": "^7.22.15",
    "@rollup/plugin-replace": "^5.0.2",
    "@toruslabs/config": "^2.0.2",
