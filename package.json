{
  "name": "@toruslabs/rss-client",
  "version": "2.0.0",
  "description": "RSS Client",
  "main": "dist/lib.cjs/index.js",
  "module": "dist/lib.esm/index.js",
  "unpkg": "dist/rssClient.umd.min.js",
  "jsdelivr": "dist/rssClient.umd.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "torus-scripts start",
    "build": "torus-scripts build",
    "release": "torus-scripts release",
    "prepack": "npm run build",
    "test:secp256k1": "CURVE=secp256k1 tsx --tsconfig tsconfig.test.json --test test/test.ts",
    "test:ed25519": "CURVE=ed25519 tsx --tsconfig tsconfig.test.json --test test/test.ts",
    "test": "npm run test:secp256k1 && npm run test:ed25519",
    "lint:ts": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "dependencies": {
    "@noble/curves": "^1.6.0",
    "@toruslabs/eccrypto": "^5.0.4",
    "@toruslabs/http-helpers": "^7.0.0",
    "bn.js": "^5.2.1",
    "elliptic": "^6.5.6",
    "fetch": "^1.1.0",
    "loglevel": "^1.9.1",
    "web3-eth-contract": "^4.6.0",
    "web3-utils": "^4.3.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/register": "^7.24.6",
    "@rollup/plugin-replace": "^5.0.7",
    "@toruslabs/config": "^2.2.0",
    "@toruslabs/eslint-config-typescript": "^3.3.3",
    "@toruslabs/torus-scripts": "^6.1.1",
    "@types/elliptic": "^6.4.18",
    "@types/mocha": "^10.0.7",
    "assert": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.50.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "mocha": "^10.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "torus",
    "nodes",
    "web3auth"
  ],
  "author": "Web3Auth",
  "license": "MIT",
  "homepage": "https://web3auth.io",
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  }
}
